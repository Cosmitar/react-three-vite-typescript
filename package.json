    "check_circular": "npx madge --circular ./src/App.tsx",
    "check_typescript": "tsc --noEmit --skipLibCheck"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
